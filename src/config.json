{
  "useMockData": true,
  "theme": true,
  "defaultTheme": "light",
  "github": true,
  "app": {
    "title": "UtileShare",
    "description": "UtileShare是一个专注于实用资源分享的网站，致力于为用户提供高质量的资源下载和分享服务"
  },
  "api": {
    "endpoints": {
      "resources": {
        "list": "/api/resources",
        "detail": "/api/resources/:id",
        "download": "/api/resources/download"
      },
      "categories": {
        "list": "/api/categories"
      }
    }
  },
  "security": {
    "enable": true,
    "sign": {
      "algo": "SHA-256",
      "secret": "CHANGE_ME_SIGN_SECRET"
    },
    "encrypt": {
      "algo": "AES-128-CBC",
      "keyBase64": "MDEyMzQ1Njc4OWFiY2RlZg==",
      "ivBase64": "YWJjZGVmMDEyMzQ1Njc4OQ=="
    },
    "transport": {
      "payloadField": "payload",
      "headerKey": "X-Utile-Payload",
      "headerValue": "encrypted"
    }
  },
  "seo": {
    "blockCrawlers": true,
    "includeTestPage": true
  }
}
